{
  "useTabs": true,
  "tabWidth": 4,
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 100,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "endOfLine": "lf",
  "overrides": [
    {
      "files": "*.md",
      "options": {
        "useTabs": false,
        "tabWidth": 2
      }
    }
  ]
}
